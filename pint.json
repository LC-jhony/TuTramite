{
    "preset": "laravel",
    "rules": {
        "declare_strict_types": true,
        "not_operator_with_successor_space": false,
        "phpdoc_separation": {
            "groups": [
                ["immutable", "once", "execute", "code", "run", "task"],
                ["covers", "uses", "testdox"]
            ]
        }
    }
}